<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html lang="en">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<link href="bootstrap/css/bootstrap-responsive.css" rel="stylesheet">
<link href="bootstrap/css/bootstrap.css" rel="stylesheet"
	type="text/css">
<title>Insert title here</title>
</head>
<body>
	<div class="row">
		<div class="span4">
			<p class="lead">1234</p>
		</div>
		<div class="span8">
			<p>
				<small>12345678</small>
			</p>
		</div>
	</div>
	<div class="row">
		<div class="span2">
			<p>
				<strong>....</strong>
			</p>
		</div>
		<div class="span8 offset2">
			<p>
				<em>heheheh</em>
			</p>
		</div>
	</div>
	<div class="container">
		<div>
			<p>container</p>
		</div>
	</div>
	<div class="container-fluid">
		<div class="span2">
			<p>00000000000000000000</p>
		</div>
		<div class="span10">
			<p class="text-right">1111111111111111111111111</p>
		</div>
	</div>
	<div>
		<abbr title="hey">hehe</abbr>
	</div>
	<div>
		<blockquote class="pull-right">
			<p>html can reference</p>
		</blockquote>
	</div>
	<div>
		<ul class="unstyled">
			<li>a</li>
			<li>b</li>
			<li>c</li>
		</ul>
		<ul class="inline">
			<li>a</li>
			<li>b</li>
			<li>c</li>
		</ul>
	</div>
	<div>
		<dl class="dl-horizontal">
			<dt>t1</dt>
			<dd>d1</dd>
			<dt>t2</dt>
			<dd>d2</dd>
		</dl>
	</div>
	<div>
		<table class="table table-bordered">
			<tr>
				<td>11</td>
				<td>12</td>
			</tr>
			<tr>
				<td>21</td>
				<td>22</td>
			</tr>
		</table>
	</div>
</body>
</html>